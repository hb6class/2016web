<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>mvc2-1</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>orgSrv1</servlet-name>
    <servlet-class>com.model1.srv.Ex01</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>orgSrv2</servlet-name>
    <servlet-class>com.model1.srv.Ex03</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>orgSrv1</servlet-name>
    <url-pattern>/ex01</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>orgSrv1</servlet-name>
    <url-pattern>/ex/01</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>orgSrv2</servlet-name>
    <url-pattern>/ex02</url-pattern>
  </servlet-mapping>
</web-app>